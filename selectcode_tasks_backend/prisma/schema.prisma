//! Depends if there is a way to set up a postgresql server with typescript 
//! -> yes => all clear // no => tell the user how to migrate the prisma schema (README.md)

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
